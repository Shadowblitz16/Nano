{
    "configurations": [
        {
            "name": "Watcom",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_cdecl="
            ],
            "compilerPath": "/usr/bin/watcom/binl/wcc",
            "cStandard": "c99",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-4 -d3 -s -wx -ms -zl -zq"
            ],
            "mergeConfigurations": false,
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}